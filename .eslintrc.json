{
    "extends": [
      "airbnb",
      "plugin:meteor/recommended"
    ],
    "plugins": [
        "react",
        "jsx-a11y",
        "import",
        "meteor",
        "mocha"
    ],
    "env": {
      "browser": true,
      "node": true,
      "mocha": true
    },
    "rules": {
      "no-mixed-operators": 0,
      "func-names": 0,
      "prefer-arrow-callback": 0,
      "import/no-extraneous-dependencies": 0,
      "import/prefer-default-export": 0,
      "react/jsx-filename-extension": 0,
      "react/prop-types": 0,
      "import/no-unresolved": [2, {"ignore": ["^meteor/"]}]
    }
}
